<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.ExampleDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.demo.entities.Example">
        <id column="id" property="id" />
        <result column="exp_title" property="expTitle" />
        <result column="exp_name" property="expName" />
        <result column="exp_type" property="expType" />
        <result column="exp_content" property="expContent" />
        <result column="pic_id" property="picId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="status" property="status" />
    </resultMap>


    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, exp_title, exp_name, exp_type, exp_content, pic_id, create_time, update_time, status
    </sql>

</mapper>
