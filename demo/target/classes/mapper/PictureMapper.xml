<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.PictureDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.demo.entities.Picture">
        <id column="id" property="id" />
        <result column="pic_title" property="picTitle" />
        <result column="pic_type" property="picType" />
        <result column="pic_url" property="picUrl" />
        <result column="create_time" property="createTime" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, pic_title, pic_type, pic_url, create_time, status
    </sql>

</mapper>
