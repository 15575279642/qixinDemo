<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.NewsDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.demo.entities.News">
        <id column="id" property="id" />
        <result column="news_title" property="newsTitle" />
        <result column="news_name" property="newsName" />
        <result column="news_type" property="newsType" />
        <result column="news_content" property="newsContent" />
        <result column="news_source" property="newsSource" />
        <result column="public_time" property="publicTime" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, news_title, news_name, news_type, news_content, news_source, public_time, create_time, update_time, status
    </sql>

</mapper>
